{
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "extends": ["eslint:recommended"],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
        "semi": "error",
        "no-console": [0],
        "indent": [ "error", 2 ]
    },
    "plugins": [
        "mocha"
    ]
}
