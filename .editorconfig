https://EditorConfig.org

root = true

[*]
end_of_line = lf
insert_final_newline = true

[*.py]
charset = utf-8
indet_style = space
indent_size = 4

[*.js]
quote_type = auto
charset = utf-8
indet_style = space
indent_size = 2

[Makefile]
indent_style = tab

[*.{json,yaml}]
indent_style = space
indent_size = 2
